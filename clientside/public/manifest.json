{
  "name": "Tab Manager",
  "description": "manage all your tabs in one go",
  "version" : "1.0.0",
  "manifest_version": 3,
  "action":{
    "default_title": "Tab Manger",
    "default_popup": "index.html"
  },
  "background": {
    "persistent": false
  },
  "permissions": [
      "storage",
      "tabs",
      "http://localhost:2000/"
  ] 
}
